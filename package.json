{
  "name": "sapphire",
  "displayName": "Sapphire",
  "description": "A minimal VSCode extension with a custom panel",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sapphire.start",
        "title": "Sapphire: Open Panel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sapphireViewContainer",
          "title": "Sapphire",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "sapphireViewContainer": [
        {
          "id": "sapphireView",
          "name": "Sapphire Panel",
          "icon": "media/icon.svg",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {},
  "devDependencies": {}
}
